  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "dev": "nodemon --exec babel-node src/server.js",
    "build": "yarn clean && yarn build-babel",
    "start": "yarn build && node ./build/server.js"
  },
  "dependencies": {
